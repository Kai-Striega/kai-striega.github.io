  z-index: -1;
  pointer-events: none;
}
div.admonition > .admonition-title::after,
.admonition > .admonition-title::after {
  position: absolute;
  left: 0.5rem;
  width: 1rem;
  height: 1rem;
  color: var(--pst-color-info);
  font: var(--fa-font-solid);
